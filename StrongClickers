local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local a = library:CreateWindow("Strong Clickers!") -- Creates the window
local b = a:CreateFolder("Autofarm") -- Creates the folder(U will put here your buttons,etc)
local c = a:CreateFolder("Eggs") -- Creates the folder(U will put here your buttons,etc)

b:Toggle("Auto Click",function(bool)
    _G.autoClicker = bool
    print(shared.toggle)
    if bool then
        autoClick()
    end
end)

b:Toggle("Auto Rebirth",function(bool)
    _G.autoRebirther = bool
    print(shared.toggle)
    if bool then
        autoRebirth()
    end
end)

local selEgg
c:Dropdown("Eggs",{"Basic Egg","Deadly Egg","Featured","Diabolos Egg","Valkyries Egg","Earthly Egg","Mountain Egg","Desert Egg","Winter Egg","Beach Egg","Ocean Egg"},true,function(value) --true/false, replaces the current title "Dropdown" with the option that t
    selEgg = value
end)

c:Toggle("Open Egg",function(bool)
    _G.autoEgger = bool
    if bool then 
        autoEgg(selEgg)
    end
end)



function autoEgg()
    spawn(function()
        while _G.autoEgger == true do
            local ohString1 = selEgg
            game:GetService("ReplicatedStorage").Remotes.HatchEgg:FireServer(ohString1)
            wait()
        end
    end)
end

function autoRebirth()
    spawn(function()
        while _G.autoRebirther == true do
            game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer()
            wait(1)
        end
    end)
end

function autoClick()
    spawn(function()
        while _G.autoClicker == true do 
            game:GetService("ReplicatedStorage").Remotes.TrainStrength:FireServer()
            wait()
        end
    end)
end
